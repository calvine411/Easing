float targetX;
float targetY;
float distX;
float distY;
float x;
float y;
float easing=.05;
PImage cheetah;

void setup(){
  size(600,600);
  x=width/2;
  y=height/2;
  cheetah = loadImage("baby_cheetah.jpg");
}

void draw(){
  background(0);
  targetX=mouseX;
  targetY=mouseY;
  distX=targetX-x;
  distY=targetY-y;
  x+=distX*easing;
  y+=distY*easing;
//  fill(0,200,200);
//  ellipse(x,y,75,75);
  imageMode(CENTER);
  image(cheetah,x,y,200,150);
  
